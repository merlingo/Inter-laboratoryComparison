@model CalCom.Web.Models.OlcumKarsilastirmalari
@{
    ViewBag.Title = "Ölçüm";
   ViewBag.Lab = "UMS";
    ViewBag.Yonetici = "Mert Nar";
    ViewBag.CalismaAlani = "Demir Çelik Kalibrasyonu";
    ViewBag.ImagePath = "../../dist/img/user2-160x160.jpg";
}
        <div class="container">
          <!-- Content Header (Page header) -->
          <section class="content-header">
            <h1>
             İşlemler
              
            </h1>
            <ol class="breadcrumb">
                <li><a href="Islemler"><i class="fa fa-dashboard"></i> İşlemler</a></li>
                <li class="active">Ölçüm</li>
            </ol>
          </section>

          <!-- Main content -->
          <section class="content"> <!-- renderBody() buraya gelecek, içerik burada değişecek hatta eğer ihtiyaç duyulursa "content-wrapper" dan da alınabilir. Çünkü her sayfa için değişecek yan menüyü sayfalara alırız.-->
            <div class="row"> 
             <div class="col-sm-3"> 
                 
                     <ul  class="nav nav-pills nav-stacked">
                        <li ><a href="TestSeç" >Test-Kayıt</a></li>
                        <li class="active"><a href="OlcumKarsilastirmalari"> Ölçüm</a></li>
                        <li><a href="Analiz">Analiz</a></li>

                    </ul>
                 
              </div> 
            <div class="col-sm-9">
                  <div class="box box-default tab-pane fade active in">
                     <div class="box-header with-border">
                     <h3 class="box-title">Karşılaştırma Ölçüm Ekranı</h3>
                     </div>
                     <div class="box-body ">
                     <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>

                                <th>Karşılaştırma </th>
                                <th>Kalibre Et</th>
                               



                            </tr>
                        </thead>
                        <tbody>
                            @foreach (CalCom.Web.Models.OlcumKar ok in Model.oklar)
                            {
                                <tr>
                                    <td> @ok.olcumAdi   </td><td>@Html.ActionLink("SEÇ", "SifreGir", new { olcId = ok.testid }, new {  style =  ok.uygunMu ?  "" : "display : none"  })</td>

                                </tr>
                            }
                        </tbody>
                    </table>
                       
                    </div><!-- /.box-body -->
                 </div><!-- /.box -->
                 
            </div>
        </div>  

          </section>  <!-- /.content -->
      
        </div><!-- /.container -->











