@model System.Collections.Generic.List<Calcom.Domain.Entities.Testler>
@{
    ViewBag.Title = "Test-Kayıt";
    ViewBag.Lab = "UMS";
    ViewBag.Yonetici = "Mert Nar";
    ViewBag.CalismaAlani = "Demir Çelik Kalibrasyonu";
    ViewBag.ImagePath = "../../dist/img/user2-160x160.jpg";
}
        <div class="container">
          <!-- Content Header (Page header) -->
          <section class="content-header">
            <h1>
             İşlemler
              
            </h1>
            <ol class="breadcrumb">
                <li class="active"><a href="Islemler"><i class="fa fa-dashboard"></i> İşlemler</a></li>
                <li class="active">Test-Kayıt</li>
            </ol>
          </section>

          <!-- Main content -->
          <section class="content"> <!-- renderBody() buraya gelecek, içerik burada değişecek hatta eğer ihtiyaç duyulursa "content-wrapper" dan da alınabilir. Çünkü her sayfa için değişecek yan menüyü sayfalara alırız.-->
            <div class="row"> 
             <div class="col-sm-3"> 
                 
                     <ul  class="nav nav-pills nav-stacked">
                        <li class="active"><a href="TestSeç" >Test-Kayıt</a></li>
                        <li><a href="OlcumKarsilastirmalari"> Ölçüm</a></li>
                        <li><a href="Analiz">Analiz</a></li>

                    </ul>
                 
              </div> 
            <div class="col-sm-9">
                  <div class="box box-default tab-pane fade active in" id="LabPanel">
                     <div class="box-header with-border">
                     <h3 class="box-title">Karşılaştırma Kayıt Ekranı</h3>
                     </div>
                     <div class="box-body ">
                      <p> Karşılaştırma kayıt işlemi için uygun olan karşılaştırmaların listesi aşağıdadır. Lütfen kayıt olmak istediğiniz karşılaştırmayı seçiniz.</p>
            <div class="table-responsive">
        
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                           
                                            <th>Karsilastirmalar</th>
                                            <th>Tur</th>
                                             <th>Konu</th>
                                             <th>OlcumSuresi</th>
                                             <th>Başlama Tarihi</th>
                                            <th>Bitiş Tarihi</th>

                                             <th>Fiyat</th>
                                             <th>ONAY</th>
                


                                                                                        
                                        </tr>
                                    </thead>
                                    <tbody>
                                      
                             

@for (int i = 0; i < Model.Count; i++ )
{
 <tr>
     
      <td>  @Html.DisplayFor(m => m[i].Karsilastirmalar.OlcumAletleri.Isim)</td>
          
     <td> @Html.DisplayFor(m => m[i].Karsilastirmalar.Tur)</td>

          <td> @Html.DisplayFor(m => m[i].Karsilastirmalar.Konu)</td>

          <td> @Html.DisplayFor(m => m[i].OlcumSuresi) gün</td>
             <td> @Html.DisplayFor(m => m[i].BaslangicZamani)</td>
          <td> @Html.DisplayFor(m => m[i].BitisZamani)</td>

        <td> @Html.DisplayFor(m => m[i].Fiyat)</td>

          <td>@Html.ActionLink("SEÇ", "TestKayit", new { Model[i].Id })</td>

 </tr>
    
}
                                         
                                    </tbody>
                                </table>
                           
                                  
                                 
                        </div>
                    </div><!-- /.box-body -->
                 </div><!-- /.box -->
                 
            </div>
        </div>  

          </section><!-- /.content -->
      
        </div><!-- /.container -->

