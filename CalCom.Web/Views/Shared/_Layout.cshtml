<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>

    @Scripts.Render("~/bundles/modernizr")
    

    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/dist/css/AdminLTE.min.css">

    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/dist/css/skins/_all-skins.min.css">
    <link rel="stylesheet" href="~/Content/SistemCss/responsive-calendar.css" >

    <!-- fullCalendar 2.2.5-->
    <link rel="stylesheet" href="~/plugins/fullcalendar/fullcalendar.min.css">
    <link rel="stylesheet" href="~/plugins/fullcalendar/fullcalendar.print.css" media="print">

</head>
<body class="skin-blue layout-boxed layout-top-nav">
        <script src="~/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Bootstrap 3.3.5 -->
    <script src="~/Scripts/bootstrap.min.js"></script>
        
    <script src="~/Scripts/responsive-calendar.js"></script>
    <!-- SlimScroll -->
    <script src="~/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/plugins/fastclick/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/dist/js/app.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/dist/js/demo.js"></script>

    <div class="wrapper">

      <header class="main-header">
        <nav class="navbar navbar-static-top">
          <div class="container">
            <div class="navbar-header">
              <a href="~/Genel/Anasayfa" class="navbar-brand"><b>LAK</b>SİS</a>
              <button type="button" class="navbar-toggle mini" data-toggle="collapse" data-target="#navbar-collapse">
                <i class="fa fa-bars"></i>
              </button>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
              <ul class="nav navbar-nav">
                 <li class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">İşlemler <span class="caret"></span></a>
                  <ul class="dropdown-menu" role="menu">
                    <li>@Html.ActionLink("İşlemler","Islemler","KarsilastirmaIslem")</li>
                    <li class="divider"></li>
                    <li><a href="~/KarsilastirmaIslem/TestSeç">Test Kayıt</a></li>
                    <li><a href="~/KarsilastirmaIslem/OlcumKarsilastirmalari">Ölçüm</a></li>
                  </ul>
                </li>
                 
                 <li>
                        @Html.ActionLink("Karşılaştırmalar","Takip","KarsilastirmaTakip")
                 </li>
                 <li>
                       @Html.ActionLink("Arşiv","Arsiv","Arsiv")

               </li>
           </ul>
            <form class="navbar-form navbar-left" role="search">
                <div class="form-group">
                  <input type="text" class="form-control" id="navbar-search-input" placeholder="Arama">
                </div>
             </form>
           </div><!-- /.navbar-collapse -->
                <!-- Navbar Right Menu -->
           <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                  <!-- Messages: style can be found in dropdown.less-->
                  <li class="dropdown messages-menu">
                    <!-- Menu toggle button -->
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                      <i class="fa fa-envelope-o"></i>
                      <span class="label label-success">@ViewBag.MesajSayisi</span>
                    </a>
                    <ul class="dropdown-menu">
                      <li class="header"> @ViewBag.MesajSayisi Mesajınız bulunmaktadır</li>
                      <li>
                        <!-- inner menu: contains the messages -->
                        <ul class="menu">
                          <li><!-- start message -->
                            <a href="~/Laboratuvar/MesajKutusu">
                              <div class="pull-left">
                                <!-- User Image -->
@*                                <img src="../../dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">*@
                              </div>
                              <!-- Message title and timestamp -->
                              <h4>
                                Support Team
                                <small><i class="fa fa-clock-o"></i> 5 mins</small>
                              </h4>
                              <!-- The message -->
                              <p>Why not buy a new awesome theme?</p>
                            </a>
                          </li><!-- end message -->
                        </ul><!-- /.menu -->
                      </li>
                      <li class="footer"><a href="~/Laboratuvar/MesajKutusu">Tüm Mesajları Göster</a></li>
                    </ul>
                  </li><!-- /.messages-menu -->

                  <!-- Notifications Menu -->
                  <li class="dropdown notifications-menu">
                    <!-- Menu toggle button -->
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                      <i class="fa fa-bell-o"></i>
                      <span class="label label-warning">1</span>
                    </a>
                    <ul class="dropdown-menu">
                      <li class="header">Bildirimler</li>
                      <li>
                        <!-- Inner Menu: contains the notifications -->
                        <ul class="menu">
                          <li><!-- start notification -->
                            <a href="#">
                            LAKSIS sistemi internet üstünden yayınlanmıştır.
                            </a>
                          </li><!-- end notification -->
                        </ul>
                      </li>
                      <li class="footer"><a href="~/Genel/Bildirimler">Tümünü Göster</a></li>
                    </ul>
                  </li>
                  <!-- Tasks Menu -->
                @*  <li class="dropdown tasks-menu">
                    <!-- Menu Toggle Button -->
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                      <i class="fa fa-flag-o"></i>
                      <span class="label label-danger">2</span>
                    </a>
                    <ul class="dropdown-menu">
                      <li class="header">Yapılacaklar Listesi</li>
                      <li>
                        <!-- Inner menu: contains the tasks -->
                        <ul class="menu">
                          <li><!-- Task item -->
                            <a href="#">
                              <!-- Task title and progress text -->
                              <h3>
                                Design some buttons
                                <small class="pull-right">20%</small>
                              </h3>
                              <!-- The progress bar -->
                              <div class="progress xs">
                                <!-- Change the css width attribute to simulate progress -->
                                <div class="progress-bar progress-bar-aqua" style="width: 20%" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">
                                  <span class="sr-only">20% Complete</span>
                                </div>
                              </div>
                            </a>
                          </li><!-- end task item -->
                        </ul>
                      </li>
                      <li class="footer">
                        <a href="#">Tüm yapılacakları göster</a>
                      </li>
                    </ul>
                  </li>*@
                  <!-- User Account Menu -->
                  <li class="dropdown user user-menu">
                    <!-- Menu Toggle Button -->
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                      <!-- The user image in the navbar-->
                      <img src="@ViewBag.ImagePath" class="user-image" alt="User Image">
                      <!-- hidden-xs hides the username on small devices so only the image appears. -->
                      <span class="hidden-xs">@ViewBag.Lab </span>
                    </a>
                    <ul class="dropdown-menu">
                      <!-- The user image in the menu -->
                      <li class="user-header">
                        <img src="../../dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                        <p>
                         @ViewBag.Lab - @ViewBag.Yonetici
                          <small> @ViewBag.CalismaAlani </small>
                        </p>
                      </li>
                      <!-- Menu Body -->
                      <li class="user-body">
                        <div class="col-xs-4 text-center">
                          <a href="~/Laboratuvar/LabTakvim">Takvim</a>
                        </div>
                        <div class="col-xs-6 col-xs-offset-2 text-center">
                          <a href="~/Laboratuvar/ReferansCihazlar">Referans Cihazlar</a>
                        </div>
                        
                      </li>
                      <!-- Menu Footer-->
                      <li class="user-footer">
                        <div class="pull-left">
                          <a href="~/Laboratuvar/Index" class="btn btn-default btn-flat">Bilgiler</a>
                        </div>
                        <div class="pull-right">
                          <a href="~/Uyelik/Cikis" class="btn btn-default btn-flat">ÇIKIŞ</a>
                        </div>
                      </li>
                    </ul>
                  </li>
                               
                </ul>
              </div><!-- /.navbar-custom-menu -->
          </div><!-- /.container-fluid -->
        </nav>
      </header>
      <div class="content-wrapper">

               @RenderBody()
      </div><!-- /.content-wrapper -->
      <footer class="main-footer">
        <div class="container">
          <div class="pull-right hidden-xs">
            <b>Version</b> 2.3.0
          </div>
          <strong>Copyright &copy; 2014-2015<a href="#">Mert NAR</a>.</strong> All rights reserved. | Supported by <a href="http://www.jera.com.tr" target="_parent">Jera Elektronik</a>
        </div><!-- /.container -->
      </footer>
        
   
    </div><!-- ./wrapper -->

    <!-- jQuery 2.1.4 -->

  </body>
</html>



