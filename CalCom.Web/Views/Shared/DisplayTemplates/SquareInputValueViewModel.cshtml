@model CalCom.Web.Models.SquareInputValueViewModel



 <div class="table-responsive table-bordered">
                                <table class="table">
                                    <thead>
                                        <tr><th>Ölçüm Noktası</th>
                                            @foreach (var m in Model.baslikPD)
                                            {
                                                <th>@m</th>
                                            }
                                        </tr>
                                    </thead>


                                    <tbody>
                                     @{ int count = Model.baslikPD.Count; }
                                         @Html.HiddenFor(x => x.olcId)   
                                           @for (int i = 0; i < Model.olcumNoktasi.Count; i++)
                                           {
                                           
                                            <tr>
                                                <td>@Model.olcumNoktasi[i]</td>

                                               @for (int j = 0; j <count; j++)
                                                {
                                                  
                                                <td>@Html.DisplayFor(x=>x.sonuclar[j,i].Deger) </td>
                                                }
                                            </tr>
                                           }
                                     
                                        
                                    </tbody>
                                </table>
                            </div>