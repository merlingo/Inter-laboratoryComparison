<script>
    var sekme = document.getElementById("karsilastirma_sekme");
    sekme.classList.add("active");
    function IslemPartial(is) {
        $.ajax(
       {
           url: '/AdminPanel/' + is,
           type: "GET",
           //data: { 'id': id, 'k': k },
           success: function (response) {
               $("#labtestsecim").html(response);
           }
       });
    }
</script>